{
    "root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true,
        "node": true
    },
    "extends": ["prettier", "plugin:import/errors", "plugin:import/warnings", "plugin:jsx-a11y/recommended"],
    "plugins": ["prettier"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2021,
        "requireConfigFile": false,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "impliedStrict": true
        }
    },
    "rules": {
        "radix": 0,
        "import": 0,
        "max-len": 0,
        "no-alert": 0,
        "camelcase": 0,
        "no-shadow": 0,
        "no-console": 0,
        "comma-dangle": 0,
        "no-return-assign": 0,
        "consistent-return": 1,
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "array-callback-return": 0,
        "prefer-promise-reject-errors": 0,
        "no-unused-vars": [
            1,
            {
                "ignoreRestSiblings": false
            }
        ],

        "import/no-cycle": 0,
        "import/prefer-default-export": 0,
        "import/no-extraneous-dependencies": 0,

        "jsx-a11y/alt-text": 0,
        "jsx-a11y/href-no-hash": [0],
        "jsx-a11y/anchor-is-valid": 0,
        "jsx-a11y/click-events-have-key-events": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "jsx-a11y/no-noninteractive-element-interactions": 0,

        "prettier/prettier": [
            2,
            {
                "printWidth": 120,
                "singleQuote": true,
                "trailingComma": "none",
                "tabWidth": 4
            }
        ]
    }
}
